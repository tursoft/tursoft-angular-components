{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ngx-photo-card": [
        "dist/ngx-photo-card"
      ],
      "ngx-photo-card/*": [
        "dist/ngx-photo-card/*"
      ],
      "tursoft-collapsible": [
        "dist/tursoft-collapsible"
      ],
      "tursoft-collapsible/*": [
        "dist/tursoft-collapsible/*"
      ],
      "text-expander": [
        "dist/text-expander"
      ],
      "text-expander/*": [
        "dist/text-expander/*"
      ],
      "tursoft-components-common": [
        "dist/tursoft-components-common"
      ],
      "tursoft-components-common/*": [
        "dist/tursoft-components-common/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}